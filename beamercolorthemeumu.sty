\mode<presentation>

\definecolor{ucugreyish}{RGB}{33,33,33}
\definecolor{ucugrey}{RGB}{33,33,33}
\definecolor{ucuwhite}{RGB}{255,255,255}
\definecolor{ucuyellow}{RGB}{255,220,0}

% Normal text
\setbeamercolor{normal text}{fg=ucuwhite}

% First page
\setbeamercolor*{title}{fg=ucuwhite,bg=gray}
\setbeamercolor*{author}{fg=ucuwhite,bg=gray}
\setbeamercolor*{date}{fg=ucuwhite,bg=gray}

% Generic frames header and footer
\setbeamercolor{titlelike}{fg=black}
\setbeamercolor*{frametitle}{fg=ucugrey}
\setbeamercolor*{framesubtitle}{fg=ucugrey}
\setbeamercolor*{title in head/foot}{fg=black}
\setbeamercolor*{author in head/foot}{fg=black}

%Blocks
\setbeamercolor{block title}{bg=ucuyellow,fg=black}
\setbeamercolor{block title alerted}{bg=ucuyellow,fg=black}
\setbeamercolor{block title example}{bg=ucuyellow,fg=black}
\setbeamercolor{block body}{bg=ucuyellow!15,fg=black}
\setbeamercolor{block body alerted}{bg=ucuyellow,fg=black}
\setbeamercolor{block body example}{bg=ucuyellow,fg=black}

% Lists
\setbeamercolor*{item}{fg=ucuyellow,bg=ucugrey}

% Quotation and quote
\setbeamercolor*{quotation}{fg=ucuwhite,bg=ucugrey}

% Verse
\setbeamercolor*{verse}{fg=ucuwhite,bg=ucugrey}

% Abstract
\setbeamercolor*{abstract}{fg=ucuwhite,bg=ucugrey}

% Table of contents 
\setbeamercolor*{structure}{fg=ucuwhite}

% Bibliography
\setbeamercolor*{bibliography entry title}{fg=ucuwhite}
\setbeamercolor*{bibliography entry author}{fg=ucuwhite}
\setbeamercolor*{bibliography entry location}{fg=ucuwhite!70}
\setbeamercolor*{bibliography entry note}{fg=ucuwhite!70}


\mode
<all>
